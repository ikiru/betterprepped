<p id="notice"><%= notice %></p>

<h1>Project Catagories</h1>

<table>
  <thead>
    <tr>
      <th>Catagory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_catagories.each do |project_catagory| %>
      <tr>
        <td><%= project_catagory.catagory %></td>
        <td><%= link_to 'Show', project_catagory %></td>
        <td><%= link_to 'Edit', edit_project_catagory_path(project_catagory) %></td>
        <td><%= link_to 'Destroy', project_catagory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project Catagory', new_project_catagory_path %>
