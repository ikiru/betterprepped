<p id="notice"><%= notice %></p>

<h1>Interviews</h1>

<table>
  <thead>
    <tr>
      <th>Interview number</th>
      <th>Position</th>
      <th>Interview type</th>
      <th>Interview question</th>
      <th>Interview comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interviews.each do |interview| %>
      <tr>
        <td><%= interview.interview_number %></td>
        <td><%= interview.position %></td>
        <td><%= interview.interview_type %></td>
        <td><%= interview.interview_question %></td>
        <td><%= interview.interview_comments %></td>
        <td><%= link_to 'Show', interview %></td>
        <td><%= link_to 'Edit', edit_interview_path(interview) %></td>
        <td><%= link_to 'Destroy', interview, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interview', new_interview_path %>
