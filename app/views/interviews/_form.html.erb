<%= simple_form_for(@interview) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :interview_number %>
    <%= f.association :position %>
    <%= f.association :interview_type %>
    <%= f.association :interview_question %>
    <%= f.input :interview_comments %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
